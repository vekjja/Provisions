        }
    },
    "editor.tokenColorCustomizations": {
        "[Vekjja]": {
            "textMateRules": [
                {
                    "scope": [
                        "meta.function.definition.cpp",
                        // "meta.body.function.definition.cpp",
                        // "entity.name.function"
                    ],
                    "settings": {
                        "foreground": "#eeeeee"
                    }
                },
                {
                    "scope": [
                        // "meta.parens.cpp",
                        // "constant.language.true.cpp",
                        // "constant.language",
                        // "variable.other.object.access.cpp",
                        // "entity.name.function"
                    ],
                    "settings": {
                        "foreground": "#ff0037"
                    }
                },
            ]
        }

{
    "platformio-ide.autoCloseSerialMonitor": true,
    "platformio-ide.disablePIOHomeStartup": true,
    "platformio-ide.toolbar": [
        {
            "text": "$(home)",
            "tooltip": "PlatformIO: Home",
            "commands": "platformio-ide.showHome"
        },
        {
            "text": "$(check)",
            "tooltip": "PlatformIO: Build",
            "commands": "platformio-ide.build"
        },
        {
            "text": "$(arrow-right)",
            "tooltip": "PlatformIO: Upload",
            "commands": "platformio-ide.upload"
        },
        {
            "text": "$(arrow-right)$(plug)",
            "tooltip": "PlatformIO: Upload and Monitor",
            "commands": "platformio-ide.uploadAndMonitor"
        },
        {
            "text": "$(plug)",
            "tooltip": "PlatformIO: Serial Monitor",
            "commands": "platformio-ide.serialMonitor"
        },
        {
            "text": "$(terminal)",
            "tooltip": "PlatformIO: New Terminal",
            "commands": "platformio-ide.newTerminal"
        },
        {
            "text": "$(trashcan)",
            "tooltip": "PlatformIO: Clean",
            "commands": "platformio-ide.clean"
        }
    ],
    "black-formatter.interpreter": [
        "/usr/bin/python3"
    ],
    "black-formatter.args": [
        "--line-length=79"
    ],
    "flake8.args": [
        "--max-line-length=120",
        "--ignore=E402,F841,F401,E302,E305"
    ],
    "go.toolsManagement.autoUpdate": true,
    "go.languageServerFlags": [
        "-rpc.trace"
    ],
    "[go]": {
        "editor.formatOnSave": true,
        "editor.formatOnSaveMode": "file",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "always"
        },
        "editor.defaultFormatter": "golang.go"
    },
    "explorer.confirmDelete": false,
    "gitlens.hovers.currentLine.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontFamily": "'FiraMono Nerd Font Propo', 'Operator Mono'",
    "editor.defaultFormatter": "vscode.css-language-features",
    "editor.glyphMargin": false,
    "editor.defaultFoldingRangeProvider": "golang.go",
    "editor.tabSize": 4,
    "editor.hover.delay": 300,
    "editor.renderWhitespace": "all",
    "editor.inlineSuggest.enabled": true,
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "mohd-akram.vscode-html-format"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnType": true,
        "editor.formatOnSave": true
    },
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "C_Cpp.default.customConfigurationVariables": {},
    "[cpp]": {
        "editor.wordBasedSuggestions": "matchingDocuments",
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true,
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[ignore]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "terminal.integrated.fontFamily": "'FiraMono Nerd Font Propo', 'OperatorMono Nerd Font Mono'",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.fontSize": 17,
    "terminal.integrated.fontSize": 13,
    "workbench.colorCustomizations": {

    "git.openRepositoryInParentFolders": "never",
    "workbench.productIconTheme": "material-product-icons",
    "[properties]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "files.associations": {
        "*.conf": "bat",
        "*.ino": "cpp",
        "*.md": "markdown",
        "*.yaml": "yaml",
        "*.yml": "ansible",
        "*.defaults": "ini"
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[hosts]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "go.installDependenciesWhenBuilding": true,
    "go.lintTool": "golint",
    "workbench.tree.stickyScrollMaxItemCount": 1,
    "editor.stickyScroll.maxLineCount": 6,
    "python.autoComplete.extraPaths": [],
    "go.formatTool": "goimports",
    "explorer.confirmPasteNative": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[liquid]": {},
    "terminal.integrated.allowedLinkSchemes": [
        "file",
        "http",
        "https",
        "mailto",
        "vscode",
        "vscode-insiders",
        "docker-desktop"
    ],
    "[dotenv]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "redhat.telemetry.enabled": false,
    "ansible.validation.lint.enabled": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.fontLigatures.featureSettings": "\"calt\" on, \"liga\" on",
    "workbench.iconTheme": "symbols",
    "editor.cursorBlinking": "smooth",
    "editor.cursorSurroundingLines": 1,
    "workbench.panel.showLabels": false,
    "symbols.hidesExplorerArrows": false,
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "scminput"
    ],
    "C_Cpp.intelliSenseEngine": "disabled",
    "editor.minimap.enabled": true,
    "remote.SSH.remotePlatform": {
        "mainnet-products-6": "linux"
    },
    "cursor.composer.cmdPFilePicker2": true,
    "ansible.lightspeed.enabled": false,
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "cursor.composer.textSizeScale": 1.15,
    "workbench.colorTheme": "Vekjja",
    "workbench.editor.editorActionsLocation": "hidden",
    "editor.minimap.size": "fit",
    "workbench.navigationControl.enabled": false,
    "workbench.editor.enablePreview": false
}